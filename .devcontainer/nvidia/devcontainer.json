{
    "name": "ROS Noetic Dev Nvidia",
    "dockerComposeFile": [
        "../../docker/docker-compose-nvidia.yaml"
    ],
    "service": "ros_noetic_nvidia",
    "workspaceFolder": "/home/dev/ros_ws",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash"
            },
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "ms-iot.vscode-ros",
                "ms-vscode.cpptools-extension-pack"
            ]
        }
    },
    "remoteUser": "dev",
    "overrideCommand": false
}